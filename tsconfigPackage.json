{
  "compilerOptions": {
    "jsx": "react-native",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": false,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "incremental": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true
  },
  "exclude": [
    "node_modules",
    "package",
    "**/**/*/__tests__",
    "dist"
  ],
  "types": [
    "jest"
  ]
}