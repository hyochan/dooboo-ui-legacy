{
  "homepage": "https://dooboolab.github.io/dooboo-ui/",
  "private": true,
  "scripts": {
    "pre": "lerna clean --yes && lerna bootstrap && lerna run build",
    "predeploy": "yarn build",
    "postinstall": "dooboolab-welcome postinstall && rm -rf storybook && cp -R .storybook storybook",
    "publish": "yarn build && cd lib && npm publish && cd ..",
    "publish:packages": "yarn pre && yarn build-packages && lerna exec -- npm publish",
    "lint": "eslint main packages stories --ext .ts,.tsx,.js,.jsx",
    "tsc": "tsc",
    "start": "expo start",
    "jest": "jest",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "start-storybook -p 6006",
    "eject": "expo eject",
    "test": "lerna run build && yarn lint && yarn tsc && jest",
    "test:post": "jest",
    "build": "rm -f ./lib/tsconfigBuild.tsbuildinfo && cp README.md ./lib/README.md && tsc --project tsconfigBuild.json",
    "postbuild": "cp -r main/__assets__ lib/__assets__",
    "build-storybook": "yarn build && yarn build-packages && rm -rf doc-build && cd doc && yarn build && cd ..",
    "deploy-storybook": "gh-pages -d doc-build",
    "storybook-server": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook",
    "build-packages": "lerna exec -- yarn build",
    "install-packages": "lerna exec -- yarn",
    "upgrade-packages": "lerna exec -- ncu -u && yarn install-packages"
  },
  "dependencies": {
    "dooboolab-welcome": "^1.2.0",
    "react-native-web-hooks": "^3.0.1",
    "react-responsive": "^8.2.0",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-react": "^7.13.13",
    "@dooboo/eslint-config": "^0.6.26",
    "@expo/webpack-config": "^0.12.66",
    "@react-native-async-storage/async-storage": "^1.15.2",
    "@storybook/addon-actions": "6.2.8",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^6.2.8",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.2.8",
    "@storybook/react-native": "5.3.25",
    "@storybook/react-native-server": "5.3.23",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-native": "^0.64",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.9",
    "@types/styled-components-react-native": "^5.1.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-expo": "^8.3.0",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.24.0",
    "expo": "^41.0.0",
    "expo-constants": "~10.1.3",
    "expo-font": "~9.1.0",
    "expo-linear-gradient": "~9.1.0",
    "gh-pages": "^3.1.0",
    "intl": "^1.2.5",
    "jest": "^26.6.3",
    "lerna": "^4.0.0",
    "react": "17.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "17.0.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.0.tar.gz",
    "react-native-get-random-values": "^1.7.0",
    "react-native-svg": "^12.1.1",
    "react-native-web": "^0.15.7",
    "react-test-renderer": "^17.0.2",
    "react-useinterval": "^1.0.2",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}
