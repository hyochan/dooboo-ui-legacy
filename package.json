{
  "homepage": "https://dooboolab.github.io/dooboo-ui/",
  "private": true,
  "scripts": {
    "pre": "lerna clean --yes && lerna bootstrap && lerna run build",
    "predeploy": "yarn build",
    "postinstall": "dooboolab-welcome postinstall && rm -rf storybook && cp -R .storybook storybook",
    "publish": "yarn build && cd lib && npm publish && cd ..",
    "publish:packages": "yarn pre && yarn build-packages && lerna exec -- npm publish",
    "lint": "eslint main packages stories --ext .ts,.tsx,.js,.jsx",
    "tsc": "tsc",
    "start": "expo start",
    "jest": "jest",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "start-storybook -p 6006",
    "eject": "expo eject",
    "test": "lerna run build && yarn lint && yarn tsc && jest",
    "build": "rm ./lib/tsconfigBuild.tsbuildinfo && cp README.md ./lib/README.md && tsc --project tsconfigBuild.json",
    "postbuild": "cp -r main/__assets__ lib/__assets__",
    "build-storybook": "yarn build && yarn build-packages && rm -rf doc-build && cd doc && yarn build && cd ..",
    "deploy-storybook": "gh-pages -d doc-build",
    "storybook-server": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook",
    "build-packages": "lerna exec -- yarn build",
    "install-packages": "lerna exec -- yarn",
    "upgrade-packages": "lerna exec -- ncu -u && yarn install-packages"
  },
  "dependencies": {
    "@react-native-community/async-storage": "~1.12.0",
    "dooboolab-welcome": "^1.2.0",
    "react-native-web-hooks": "^3.0.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-react": "^7.12.10",
    "@dooboo/eslint-config": "^0.6.14",
    "@expo/webpack-config": "^0.12.52",
    "@storybook/addon-actions": "6.1.11",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^6.1.11",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.11",
    "@storybook/react-native": "5.3.23",
    "@storybook/react-native-server": "5.3.23",
    "@testing-library/react-native": "^7.1.0",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-native": "^0.63",
    "@types/react-test-renderer": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-expo": "^8.3.0",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.17.0",
    "expo": "^40.0.0",
    "expo-constants": "~9.3.5",
    "expo-font": "~8.4.0",
    "expo-linear-gradient": "~8.4.0",
    "gh-pages": "^3.1.0",
    "intl": "^1.2.5",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "react": "16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "17.0.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.0.tar.gz",
    "react-native-get-random-values": "^1.5.1",
    "react-native-svg": "^12.1.0",
    "react-native-web": "^0.14.10",
    "react-test-renderer": "^17.0.1",
    "react-useinterval": "^1.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}